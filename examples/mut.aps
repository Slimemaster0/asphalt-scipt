# Create the variable "mut_var" and print it;
new("mut_var", yup, 69);
printf("First time: ", &mut_var, "\n");

# Mutate the variable "mut_var" and print it;
mut("mut_var", 420);
printf("Second time: ", &mut_var, "\n");

# Mutate the variable "mut_var" again but change the type to String and print it;
mut("mut_var", "nice\n");
printf(&mut_var);
